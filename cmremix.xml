<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- CMRemix Github remote -->
  <remote  name="cmR"
           fetch="https://github.com/CMRemix/" />

  <!-- Generic CMRemix ssh Bitbucket remote -->
  <remote  name="private-bb"
           fetch="ssh://git@bitbucket.org/CMRemix" />

  <remote name = "uber"
          revision = "master"
          fetch = "https://github.com/UBERROMS" />

   <!-- Generic CMRemix Github remote -->
  <remote  name="bb"
           fetch="https://bitbucket.org/" />

  <!-- Generic CMRemix GitLab remote -->
  <remote  name="gl"
           fetch="https://gitlab.com/CMRemix" />

  <!-- Omni Rom Github remote -->
  <remote  name="omni"
           fetch="https://github.com/omnirom"
           review="gerrit.omnirom.org" />

  <!-- CMRemix Platform Manifest -->
  <project path="CMRemix" name="CMRemix" remote="cmR" revision="cm-14.1" />

  <!-- CMRemix Build -->
  <project path="build" name="android_build" remote="cmR" revision="cm-14.1" >
    <copyfile src="core/root.mk" dest="Makefile" />
  </project>

  <!-- Base Project -->
  <remove-project name="LineageOS/android_art" />
  <project path="art" name="android_art" remote="cmR" revision="cm-14.1" />
  <remove-project name="LineageOS/android_bionic" />
  <project path="bionic" name="android_bionic" remote="cmR" revision="cm-14.1" />

   <!-- External -->
  <project path="external/DUtils" name="android_external_DUtils" remote="cmR" revision="cm-14.1" />
  <project path="external/google" name="android_external_google" remote="cmR" revision="cm-14.1" />

   <!-- Frameworks -->
  <project path="frameworks/av" name="android_frameworks_av" remote="cmR" revision="cm-14.1" />
  <project path="frameworks/base" name="android_frameworks_base" remote="cmR" revision="cm-14.1" />
  <project path="frameworks/native" name="android_frameworks_native" remote="cmR" revision="cm-14.1" />
 <project path="frameworks/opt/cards" name="frameworks_opt_cards" remote="cmR" revision="cm-14.1" />

  <!-- Package Apps -->
  <remove-project name="LineageOS/android_packages_apps_Contacts" />
  <project path="packages/apps/Contacts" name="android_packages_apps_Contacts" remote="cmR" revision="cm-14.1" />
  <remove-project name="LineageOS/android_packages_apps_ContactsCommon" />
  <project path="packages/apps/ContactsCommon" name="android_packages_apps_ContactsCommon" remote="cmR" revision="cm-14.1" />
  <remove-project name="LineageOS/android_packages_apps_Dialer" />
  <project path="packages/apps/Dialer" name="android_packages_apps_Dialer" remote="cmR" revision="cm-14.1" />
  <project path="packages/apps/DUI" name="android_packages_apps_DUI" remote="cmR" revision="cm-14.1" />
  <remove-project name="LineageOS/android_packages_apps_ExactCalculator" />
  <project path="packages/apps/ExactCalculator" name="android_packages_apps_ExactCalculator" remote="cmR" revision="cm-14.1" />
  <remove-project name="LineageOS/android_packages_apps_DeskClock" />
  <project path="packages/apps/DeskClock" name="android_packages_apps_DeskClock" groups="pdk-fs" remote="cmR" revision="cm-14.1" />
  <remove-project name="LineageOS/android_packages_apps_PhoneCommon" />
  <project path="packages/apps/PhoneCommon" name="android_packages_apps_PhoneCommon" remote="cmR" revision="cm-14.1" />
  <remove-project name="LineageOS/android_packages_apps_PackageInstaller" />
  <project path="packages/apps/PackageInstaller" name="android_packages_apps_PackageInstaller" remote="cmR" revision="cm-14.1" />
  <remove-project name="LineageOS/android_packages_services_Telephony" />
  <project path="packages/services/Telephony" name="android_packages_services_Telephony" remote="cmR" revision="cm-14.1" />
  <project path="packages/services/ThemeInterfacer" name="android_packages_services_ThemeInterfacer" remote="cmR" revision="cm-14.1" />
  <project path="packages/apps/Settings" name="android_packages_apps_Settings" remote="cmR" revision="cm-14.1" />
  <project path="packages/apps/CMRemixUpdater" name="android_packages_apps_CMRemixUpdater" remote="cmR" revision="cm-14.1" />
  <project path="packages/apps/CMParts" name="android_packages_apps_CMParts" remote="cmR" revision="cm-14.1" />
  <remove-project name="LineageOS/android_packages_services_Telecomm" />
  <project path="packages/services/Telecomm" name="android_packages_services_Telecomm" remote="cmR" revision="cm-14.1" />
  <remove-project name="LineageOS/android_packages_apps_Messaging" />
  <project path="packages/apps/Messaging" name="android_packages_apps_Messaging" remote="cmR" revision="cm-14.1" />
  <remove-project name="LineageOS/android_packages_apps_LockClock" />
  <project path="packages/apps/LockClock" name="android_packages_apps_LockClock" remote="cmR" revision="cm-14.1" />

  <!-- System -->
  <remove-project name="LineageOS/android_system_core" />
  <project path="system/core" name="android_system_core" remote="cmR" revision="cm-14.1" />
  <remove-project name="LineageOS/android_system_sepolicy" />
  <project path="system/sepolicy" name="android_system_sepolicy" groups="pdk" remote="cmR" revision="cm-14.1" />

  <!-- Mics -->
  <project path="CHANGELOGS" name="CHANGELOGS" remote="cmR" revision="cm-14.1" />
  <project path="device/qcom/sepolicy" name="android_device_qcom_sepolicy" remote="cmR" revision="cm-14.1" />
  <remove-project name="LineageOS/android_hardware_libhardware" />
  <project path="hardware/libhardware" name="android_hardware_libhardware" groups="pdk" remote="cmR" revision="cm-14.1" />
  <project path="device/qcom/common" name="android_device_qcom_common" remote="cmR" revision="cm-14.1" />

  <!-- OmniRom additions -->
  <project name="packages_apps_OmniStyle" path="packages/apps/OmniStyle" remote="cmR" revision="cm-14.1" />
  <project name="packages_services_OmniJaws" path="packages/services/OmniJaws" remote="cmR" revision="cm-14.1" />
  <project path="packages/apps/OmniSwitch" name="android_packages_apps_OmniSwitch" remote="cmR" revision="cm-14.1" />

  <!-- UBER Linaro Toolchains -->
  <project path="prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-6.x" name="uberroms/aarch64-linux-android-6.x" remote="bb" revision="master" />
  <project path="prebuilts/gcc/linux-x86/arm/arm-linux-androideabi-6.0" name="uberroms/arm-linux-androideabi-6.x" remote="bb" revision="master" />
  <project path="prebuilts/gcc/linux-x86/arm/arm-linux-androideabi-4.9" name="uber_arm-linux-android-4.9.x" remote="cmR" revision="master" />
  <project path="prebuilts/gcc/linux-x86/arm/arm-eabi-6.0" name="uberroms/arm-eabi-6.x" remote="bb" revision="master" />
  <project path="prebuilts/clang/host/linux-x86/3.9" name="uberroms/clang_linux-x86_3.x" remote="bb" revision="master" />
  <project path="prebuilts/clang/host/linux-x86/5.0" name="uberroms/clang_linux-x86_5.x" remote="bb" revision="master" />

  <!-- Vendors -->
  <remove-project name="LineageOS/cm_platform_sdk" />
  <project path="vendor/cmsdk" name="LineageOS/cm_platform_sdk" />
  <project path="vendor/cmremix" name="android_vendor_CMRemix" remote="cmR" revision="cm-14.1" >
  <copyfile src="tools/build-cmremix.sh" dest="build-cmremix.sh" />
  </project>

</manifest>
